@model System.Collections.Generic.List<Internship.Models.User>


<br />
<h4>User Management</h4>



<table class="table table-striped">
    <tr>
        <th>Actions</th>
        <th>Name</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model) 
    {
    <tr>
        <td>
            <a href="~/Usermanagement/edit/@user.Id" class="btn btn-into btn-sm">Edit</a>
            <a href="~/Usermanagement/delete/@user.Id" class="btn btn-danger btn-sm">Delete</a>
        </td>
        <td>@user.FirstName</td>
        <td>@user.Email</td>
    </tr>
    }
</table>